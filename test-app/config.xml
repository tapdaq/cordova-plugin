<?xml version='1.0' encoding='utf-8'?>
<widget id="com.tapdaq.tdcordovaproductiontest" version="4.4.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>Tapdaq Cordova SDK test application</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="support@tapdaq.com">
        Tapdaq
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <config-file target="AndroidManifest.xml" parent="/manifest/application">
            <meta-data android:name="com.google.android.gms.ads.APPLICATION_ID" android:value="ADMOB_APP_ID"/>
        </config-file>
    </platform>
    <preference name="android-minSdkVersion" value="22" />
    <preference name="AndroidXEnabled" value="true" />

    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
